{
  "name": "mk-base-adapter",
  "version": "1.3.0",
  "description": "Basic CRUD adapter logic for express requests",
  "main": "lib/bookshelf-adapter.js",
  "repository": "https://github.com/mkastner/mk-base-adapter.git",
  "author": "John Galt",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production rimraf dist/* && webpack --config build/webpack.config.js",
    "db:bookshelf:create": "NODE_ENV=test node test/bookshelf/db/create.js",
    "db:bookshelf:migration": "NODE_ENV=test knex migrate:make --knexfile=knexfile.js",
    "db:bookshelf:migrate": "NODE_ENV=test knex migrate:latest --knexfile=knexfile.js",
    "db:bookshelf:rollback": "NODE_ENV=test knex migrate:rollback --knexfile=test/bookshelf/db/knexfile.js",
    "test": "yarn test:adapter && yarn test:plugin",
    "test:adapter": "yarn test:adapter:bookshelf && yarn test:adapter:mongoose",
    "test:adapter:bookshelf": "NODE_ENV=test node test/bookshelf/bookshelf-adapter.test.js",
    "test:adapter:mongoose": "NODE_ENV=test node --trace-warnings test/mongoose/mongoose-adapter.test.js",
    "test:adapter:mongoose-legacy": "NODE_ENV=test node test/mongoose/mongoose-legacy-adapter.test.js",
    "test:plugin": "yarn test:plugin:batch-update:bookshelf && yarn test:plugin:touch:bookshelf && yarn test:plugin:asset:bookshelf",
    "test:plugin:batch-update:bookshelf": "NODE_ENV=test node test/bookshelf/batch-update-plugin.test.js",
    "test:plugin:touch:bookshelf": "NODE_ENV=test node test/bookshelf/touch-plugin.test.js",
    "test:plugin:asset:bookshelf": "NODE_ENV=test node test/bookshelf/asset-plugin.test.js",
    "test:plugin:single-asset:bookshelf": "NODE_ENV=test node test/bookshelf/single-asset-plugin.test.js",
    "test:util:bookshelf": "yarn test:util:vuex-base-store:bookshelf",
    "test:util:vuex-base-store:bookshelf": "NODE_ENV=test node test/bookshelf/vuex-base-store.test.js | tap-spec"
  },
  "peerDependencies": {
    "axios": "^0.x",
    "qs": "^6.x",
    "vue": "^2.x"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.1.0",
    "bookshelf-virtuals-plugin": "^0.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.5.1",
    "express": "^4.18.1",
    "knex": "^0.20.8",
    "mk-adapter-test-helpers": "https://github.com/mkastner/mk-adapter-test-helpers.git#v0.0.9",
    "mk-deconstruct-base64": "https://github.com/mkastner/mk-deconstruct-base64.git#v0.0.4",
    "mk-env-vars": "https://github.com/mkastner/mk-env-vars.git#v0.0.10",
    "mk-graceful-stat": "https://github.com/mkastner/mk-graceful-stat.git#v0.0.4",
    "mongo-connect": "^0.0.6",
    "mongoose": "^6.5.2",
    "mongoose-paginate-v2": "^1.7.0",
    "mysql": "^2.17.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.2",
    "vue": "^2.6.11",
    "vue-eslint-parser": "^6.0.4",
    "vue-loader": "^15.8.3",
    "vuex": "^3.1.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "mk-image-converter": "https://github.com/mkastner/mk-image-converter.git#v0.0.15",
    "mk-log": "https://github.com/mkastner/mk-log.git#v0.0.14",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "pluralize": "^7.0.0"
  }
}
